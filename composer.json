    "keywords": [
        "framework",
        "laravel",
        "wordpress"
    ],
    "license": "MIT",
    "homepage": "https://github.com/laraish/options",
    "support": {
        "issues": "https://github.com/laraish/options/issues",
        "source": "https://github.com/laraish/options"
    },
    "authors": [
        {
            "name": "yaquawa",
            "email": "yaquawa@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/emboldagency/contracts"
        },
        {
            "type": "vcs",
            "url": "https://github.com/emboldagency/support"
        }
    ],
    "require": {
